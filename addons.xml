<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.video.htvonlinevn"
       name="htvonlinevn"
       version="1.1.2"
       provider-name="aRookie">
  <requires>
        <import addon="xbmc.python" version="2.1.0" optional="true" />
		<import addon="script.module.simplejson" />
		<import addon="script.module.t0mm0.common" version="1.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>http://www.htvonline.com.vn</summary>
    <description>Watch free TV at http://www.htvonline.com.vn</description>
	<disclaimer lang="en">I am not responsible for the contents of these streams, which are pulled from online provider(s). The streams do not represent my views or opinions in any way.</disclaimer>
    <platform>all</platform>
  </extension>
</addon>

<addon id="repository.daveln" name="DaveLN Repository" version="1.0" provider-name="DaveLN">
	<extension point="xbmc.addon.repository" name="DaveLN Addon Repository">
		<info compressed="false">https://github.com/daveln/repository.daveln/raw/master/addons.xml</info>
		<checksum>https://github.com/daveln/repository.daveln/raw/master/addons.xml.md5</checksum>
		<datadir zip="true">https://github.com/daveln/repository.daveln/raw/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>XBMC add-ons</summary>
		<description>Go though the add-ons and see what you like</description>
		<platform>all</platform>
	</extension>
</addon>
</addons>
